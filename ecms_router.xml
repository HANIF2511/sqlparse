	<request>
		<request_name>SET:TEMPBLOCK:CLEAR</request_name>
		<xsls>
			<xsl_name>ContractSearch.xslt</xsl_name>
			<xsl_name>ContractWritSuspend.xslt</xsl_name>
		</xsls>
	</request>
	<request>
		<request_name>SET:SIM</request_name>
		<xsls>
			<xsl_name>ContractSearch.xslt</xsl_name>
			<xsl_name>contractDevicesRead.xslt</xsl_name>
